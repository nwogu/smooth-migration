{
    "name": "nwogu/smooth-migration",
    "description": "Flexible way to manage Laravel Migrations",
    "authors": [
        {
            "name": "Gabriel Nwogu",
            "email": "nwogugabriel@gmail.com"
        }
    ],
    "license": "MIT",
    "keywords": ["laravel", "migration", "update migrations", "manage migrations", "artisan migrate"],
    "require": {
        "laravel/framework": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Nwogu\\SmoothMigration\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nwogu\\SmoothMigration\\Providers\\SmoothMigrationProvider",
                "Nwogu\\SmoothMigration\\Providers\\MakeMigrationProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
